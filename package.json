{
  "name": "shov",
  "version": "3.10.0",
  "description": "Shov CLI - Ship Production Backends in Seconds, Not Weeks with vector search and real-time streaming",
  "main": "dist/index.js",
  "bin": {
    "shov": "./bin/shov.js"
  },
  "scripts": {
    "build": "esbuild src/index.js --bundle --platform=node --outfile=dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\"",
    "postinstall": "node postinstall.js"
  },
  "keywords": [
    "database",
    "key-value",
    "edge",
    "cli",
    "storage",
    "cloudflare",
    "durable-objects"
  ],
  "author": "Shov Team",
  "license": "MIT",
  "homepage": "https://shov.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/shovdev/shov-cli.git"
  },
  "bugs": {
    "url": "https://github.com/shovdev/shov-cli/issues"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "eventsource": "^4.0.0",
    "execa": "^8.0.1",
    "file-type": "^21.0.0",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/",
    "bin/",
    "src/index.js",
    "src/config.js",
    "README.md",
    "postinstall.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "esbuild": "^0.25.9"
  }
